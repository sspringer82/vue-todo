    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "server": "json-server --watch data.json"
  },
  "dependencies": {
    "json-server": "^0.14.2",
    "vue": "^2.6.4",
    "vue-router": "^3.0.2",
    "vuelidate": "^0.7.4",
    "vuex": "^3.1.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.4.0",
    "@vue/cli-plugin-eslint": "^3.4.0",
